          <TableColumn fx:id="clmName" prefWidth="181.0" text="Name" />
          <TableColumn fx:id="clmScreenTime" prefWidth="79.0" text="Screen Time" />
            <TableColumn fx:id="clmEpisodes" prefWidth="58.0" text="Episodes" />
            <TableColumn fx:id="clmActor" minWidth="2.0" prefWidth="229.0" text="Actor" />
        </columns>
      </TableView>
      <Button fx:id="btnScreen" layoutX="234.0" layoutY="298.0" mnemonicParsing="false" onAction="#btnScreenClicked" prefHeight="56.0" prefWidth="132.0" stylesheets="@styles.css" text="Screen Time">
         <font>
            <Font name="Chiller" size="12.0" />
         </font></Button>
      <Label fx:id="lblCredit" layoutY="382.0" prefHeight="19.0" prefWidth="163.0" text="Created by: Mason Douglas" />
