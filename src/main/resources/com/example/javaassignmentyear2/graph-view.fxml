<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@styles.css" fx:controller="com.example.javaassignmentyear2.Controllers.GraphViewController">
   <children>
      <BarChart fx:id="screenTimGrph" layoutX="-1.0" prefHeight="337.0" prefWidth="600.0" title="Screen Time By Character">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="btnTableView" layoutX="264.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnTableViewClicked" text="Table View" />
   </children>
</AnchorPane>
