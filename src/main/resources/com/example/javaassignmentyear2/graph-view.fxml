<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchor" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaassignmentyear2.Controllers.GraphViewController">
   <children>
      <BarChart fx:id="screenTimGrph" layoutX="1.0" prefHeight="337.0" prefWidth="600.0" title="Screen Time By Character">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="btnTableView" layoutX="239.0" layoutY="341.0" mnemonicParsing="false" onAction="#btnTableViewClicked" prefHeight="45.0" prefWidth="124.0" text="Table View" />
      <Label fx:id="lblCredit" layoutX="1.0" layoutY="386.0" prefHeight="19.0" prefWidth="163.0" text="Created by: Mason Douglas" />
   </children>
</AnchorPane>
